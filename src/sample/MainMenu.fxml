<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="MainMenu" prefHeight="900.0" prefWidth="600.0" style="-fx-background-color: #663399;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainMenuController">
   <children>
      <Button fx:id="StartBtn" layoutX="248.0" layoutY="381.0" mnemonicParsing="false" onAction="#pressStartGame" text="Start Game" />
      <Label fx:id="TitleLbl" layoutX="89.0" layoutY="162.0" prefHeight="103.0" prefWidth="413.0" rotate="-3.8" text="Snake Versus Block Game" textFill="WHITE">
         <font>
            <Font name="Brush Script MT Italic" size="48.0" />
         </font></Label>
      <Button fx:id="ResumeBtn" layoutX="237.0" layoutY="545.0" mnemonicParsing="false" text="Resume Game" />
      <Button fx:id="LeaderboardBtn" layoutX="243.0" layoutY="707.0" mnemonicParsing="false" text="Leaderboard" />
   </children>
</AnchorPane>
